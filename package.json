{
  "name": "machine-desktop-gui",
  "version": "0.1.0",
  "description": "An Electron based fat client for working with graphs.",
  "main": "src/main.js",
  "scripts": {
    "start": "NODE_ENV=production electron main.js",
    "start-hot": "NODE_ENV=development electron main.js",
    "hot-deploy": "./node_modules/.bin/webpack-dev-server",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "docs": "./node_modules/.bin/jsdoc --recurse  -d docs src/",
    "test": "NODE_ENV=test NODE_PATH=. BABEL_DISABLE_CACHE=1 node_modules/mocha/bin/mocha",
    "cov": "BABEL_DISABLE_CACHE=1 ./node_modules/.bin/istanbul cover --report text --report html _mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sholloway/graph-cockpit.git"
  },
  "keywords": [
    "Graph",
    "notes",
    "analysis"
  ],
  "author": "Samuel D. Holloway",
  "license": "MIT",
  "private": "true",
  "bugs": {
    "url": "https://github.com/sholloway/graph-cockpit/issues"
  },
  "homepage": "https://github.com/sholloway/graph-cockpit#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "css-loader": "^0.25.0",
    "electron": "^1.4.6",
    "electron-packager": "^8.2.0",
    "electron-rebuild": "^1.4.0",
    "enzyme": "^2.6.0",
    "ignore-styles": "^5.0.1",
    "isparta": "^4.0.0",
