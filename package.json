  "description": "An Electron based fat client for working with graphs.",
  "main": "src/main.js",
  "scripts": {
    "start-client": "NODE_ENV=production electron main.js",
    "start-client-dev": "NODE_ENV=development electron main.js",
    "start-server": "./node_modules/.bin/webpack-dev-server",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "docs": "./node_modules/.bin/jsdoc --recurse  -d docs src/",
    "test": "NODE_ENV=test NODE_PATH=. BABEL_DISABLE_CACHE=1 node_modules/mocha/bin/mocha",
    "cov": "BABEL_DISABLE_CACHE=1 ./node_modules/.bin/istanbul cover --report text --report html _mocha"
  },
  "repository": {
    "type": "git",
