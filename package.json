    "jsdoc": "^3.4.3",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-webpack": "^0.7.0",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^15.3.2",
